{
  "name": "server",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ts:watch": "tsc -w",
    "node:dev": "cross-env NODE_ENV=development nodemon",
    "start": "concurrently \"yarn ts:watch\" \"yarn node:dev\"",
    "build": "tsc --build && cross-env NODE_ENV=production node build/index.js"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/helmet": "^4.0.0",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
