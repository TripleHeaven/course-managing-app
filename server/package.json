{
  "name": "server",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ts:watch": "env-cmd -f ../.env  tsc -w",
    "node:dev": "NODE_ENV=development env-cmd -f ../.env  nodemon",
    "start": "concurrently \"yarn ts:watch\" \"yarn node:dev\"",
    "build": "tsc --build && cross-env NODE_ENV=production node build/index.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "mercedlogger": "^1.0.1",
    "mongoose": "^6.8.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
